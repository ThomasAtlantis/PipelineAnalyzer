{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "punctuation.definition.dictionary.begin.pipeline",
                "settings": {
                    "foreground": "#ddc871"
                }
            },
            {
                "scope": "punctuation.definition.dictionary.end.pipeline",
                "settings": {
                    "foreground": "#ddc871"
                }
            },
            {
                "scope": "punctuation.terminator.statement.pipeline",
                "settings": {
                    "foreground": "#ddc871"
                }
            },
            {
                "scope": "constant.numeric.pipeline",
                "settings": {
                    "foreground": "#b7d07a"
                }
            },
            {
                "scope": "punctuation.separator.colon.pipeline",
                "settings": {
                    "foreground": "#ddc871"
                }
            },
            {
                "scope": "keyword.other.pipeline",
                "settings": {
                    "foreground": "#2775b6",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "keyword.control.pipeline",
                "settings": {
                    "foreground": "#17CBE7",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "punctuation.separator.range.pipeline",
                "settings": {
                    "foreground": "#ddc871"
                }
            },
            {
                "scope": "keyword.control.preprocess.pipeline",
                "settings": {
                    "foreground": "#05E2C0",
                }
            },
            {
                "scope": "comment.line.double-slash.pipeline",
                "settings": {
                    "foreground": "#708500",
                }
            },
            {
                "scope": "string.quoted.double.pipeline",
                "settings": {
                    "foreground": "#90DD69",
                    "fontStyle": "underline"
                }
            }
        ]
    }
}